{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "exclude": [
    "coverage/**",
    "test/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "cypress/**"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
} 